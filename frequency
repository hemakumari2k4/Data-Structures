#include<stdio.h>
int main() {
    int arr[]={10,20,10,20,30,20,40};
    int size=sizeof(arr)/sizeof(arr[0]);
    int element=20;
    int frequency=0;
    int i;
	for(i=0;i<size;i++) {
        if(arr[i]==element) {
            frequency++;
        }
    }
	printf("Frequency of %d: %d\n",element,frequency);
	return 0;
}
